{
  "name": "react-native-login-signup-screens",
  "version": "1.0.0",
  "description": "A reusable React Native UI library for login and signup screens",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "lib",
    "src",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "prepare": "bob build",
    "build": "bob build",
    "clean": "rm -rf lib",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react-native",
    "login",
    "signup",
    "authentication",
    "ui",
    "screens"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iCoderProgrammer/react-native-login-signup-screens.git"
  },
  "homepage": "https://github.com/iCoderProgrammer/react-native-login-signup-screens#readme",
  "bugs": {
    "url": "https://github.com/iCoderProgrammer/react-native-login-signup-screens/issues"
  },
  "author": "Prem Gupta",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "react-native-builder-bob": "^0.20.0",
    "typescript": "^5.1.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
